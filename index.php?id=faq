	<script>
		$(document).ready(function()
		{$('a[href*=#]').bind("click", function(e)
		{var anchor = $(this);
		$('html, body').stop().animate(
		{scrollTop: $(anchor.attr('href')).offset().top}, 500);
		e.preventDefault();});return false;});
	</script>
	<script>
		$(window).scroll(function()
		{if ($(this).scrollTop() < 100)
		{$('#back-t').css({'display': 'block'});}
		if ($(this).scrollTop() > 100)
		{$('#back-t').css({'display': 'none'});}});	
	</script>
	<script>
		$(window).scroll(function()
		{if ($(this).scrollTop() < 100)
		{$('#scroll-t').css({'display': 'none'});}
		if ($(this).scrollTop() > 100)
		{$('#scroll-t').css({'display': 'block'});}});	
	</script>
	<script>
		function search(string)
		{var xmlhttp;
		if(window.XMLHttpRequest)
		{xmlhttp = new XMLHttpRequest();}
		else{xmlhttp = new ActiveXObject("XMLHTTP");}
		xmlhttp.onreadystatechange = function()
		{if(xmlhttp.readyState == 4 && xmlhttp.status == 200)
		{document.getElementById("search").innerHTML = xmlhttp.responseText;}}
		xmlhttp.open("GET", "search.php?s="+string, true);
		xmlhttp.send(null);}
	</script>
